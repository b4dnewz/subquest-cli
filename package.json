{
  "name": "subquest-cli",
  "version": "0.0.3",
  "description": "The cli module for subquest scanner.",
  "homepage": "",
  "author": {
    "name": "Filippo Conti",
    "email": "filippo@codekraft.it",
    "url": ""
  },
  "bin": {
    "subquest": "lib/index.js"
  },
  "files": ["lib"],
  "main": "lib/index.js",
  "keywords": ["subquest", "subdomain", "cli-tool"],
  "devDependencies": {
    "coveralls": "^3.0.0",
    "nsp": "^3.1.0",
    "eslint": "^4.16.0",
    "prettier": "^1.10.2",
    "husky": "^0.14.3",
    "lint-staged": "^6.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-prettier": "^2.5.0",
    "eslint-config-xo": "^0.19.0"
  },
  "scripts": {
    "prepublish": "nsp check",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.js": ["eslint --fix", "git add"],
    "*.json": ["prettier --write", "git add"]
  },
  "eslintConfig": {
    "extends": ["xo", "prettier"],
    "env": {
      "node": true
    },
    "rules": {
      "no-useless-escape": ["off"],
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "printWidth": 90
        }
      ]
    },
    "plugins": ["prettier"]
  },
  "repository": "b4dnewz/subquest-cli",
  "license": "MIT",
  "dependencies": {
    "commander": "^2.13.0",
    "subquest": "^1.5.2"
  }
}
