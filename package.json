{
  "name": "subquest-cli",
  "version": "0.0.1",
  "description": "The cli module for subquest scanner.",
  "homepage": "",
  "author": {
    "name": "Filippo Conti",
    "email": "filippo@codekraft.it",
    "url": ""
  },
  "bin": {
    "subquest": "lib/index.js"
  },
  "files": [
    "lib"
  ],
  "main": "lib/index.js",
  "keywords": [
    "subquest",
    "subdomain",
    "cli-tool"
  ],
  "devDependencies": {
    "coveralls": "^2.13.1",
    "nsp": "^2.6.3",
    "eslint": "^4.1.0",
    "prettier": "^1.7.0",
    "husky": "^0.14.3",
    "lint-staged": "^4.2.1",
    "eslint-config-prettier": "^2.4.0",
    "eslint-plugin-prettier": "^2.2.0",
    "eslint-config-xo": "^0.18.2",
    "jest": "^21.0.0",
    "jest-cli": "^21.0.0"
  },
  "scripts": {
    "prepublish": "nsp check",
    "pretest": "eslint .",
    "precommit": "lint-staged",
    "test": "jest"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "no-useless-escape": [
        "off"
      ],
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "printWidth": 90
        }
      ]
    },
    "plugins": [
      "prettier"
    ]
  },
  "repository": "b4dnewz/subquest-cli",
  "jest": {
    "testEnvironment": "node"
  },
  "license": "MIT",
  "dependencies": {
    "commander": "^2.11.0",
    "subquest": "^1.3.0"
  }
}
